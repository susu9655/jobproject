<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="job.data.faq.FaqMapper">
<select id="getFaqData" resultType="faq">
	select * from faq order by num desc
</select>
<select id="getSearchFaq" resultType="faq" parameterType="String">
	select * from faq where content like CONCAT('%',#{content},'%')
</select>
<select id="getFaqContent" resultType="faq">
	select * from faq where num=#{num}
</select>
</mapper>