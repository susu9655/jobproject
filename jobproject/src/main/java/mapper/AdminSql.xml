<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="job.data.admin.AdminMapper">
	
	<select id="loginCheckofAdmin" parameterType="HashMap" resultType="int">
		select count(*) from admin where adminid=#{adminid} and adminpass=#{adminpass}
	</select>
	
	

</mapper>